generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id          Int      @id @default(autoincrement())
  title       String
  company     String
  location    String
  url         String
  source      String
  postedAt    DateTime @default(now())
}
